{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && `npm run buildcss:release`",
    "preview": "vite preview",
    "buildcss:dev": "cross-env TAILWIND_MODE=build postcss ./css/style.css -o ./dist/css/app.css",
    "buildcss:release": "cross-env NODE_ENV=production postcss ./css/style.css -o ./dist/css/app.css"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.2",
    "vite": "^2.9.15"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "alpinejs": "^3.10.5",
    "daisyui": "^2.38.1",
    "htmx.org": "^1.8.4",
    "markdown-it": "^13.0.1",
    "watch": "^0.13.0"
  }
}
